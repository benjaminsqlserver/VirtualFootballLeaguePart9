                          "name": "col5",
                          "type": "col"
                        }
                      ],
                      "name": "row2",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "fldPhoto",
                              "name": "label3",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Photo",
                              "type": "label"
                            }
                          ],
                          "name": "col6",
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "fldPhoto",
                              "type": "file-input",
                              "value": "${user.Photo}"
                            }
                          ],
                          "name": "col7",
                          "type": "col"
                        }
                      ],
                      "name": "row3",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "name": "button0",
                              "text": "Register",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "name": "button1",
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "name": "col8",
                          "type": "col"
                        }
                      ],
                      "name": "row4",
                      "type": "row"
                    }
                  ],
                  "data": "${user}",
                  "itemType": "ApplicationUser",
                  "method": "post",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${user != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "user",
        "type": "set",
        "value": "new ApplicationUser();",
        "valueType": "ApplicationUser"
      }
    ]
  },
  "layout": "Main",
  "name": "Register Application User"
}